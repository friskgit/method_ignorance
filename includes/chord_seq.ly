chords_da_pt = {
    \accidentalStyle "dodecaphonic"
    \time 6/4
    \mark \markup {\sans \fontsize #9 \raise #14 \box \bold "2" }
    <<
  	{
	    <d f>4. <f a>4. <a, c e>2.
	} \\
  	{
	    r4 <c e>2~ <c e>4. g4.-> 
	}
    >>
    |% em
%%%%%%%%%% 2 4/4 %%%%%%%%%%%
    \time 4/4
    \tuplet 5/4 {
	<bf,d>8 <f a>4 <d bf a>4~
    }
    <d bf a>4
    <b, f d>4
    | % D7
%%%%%%%%%% 3 %%%%%%%%%%%
    <c ef f>4.
    <c bf f>4.
    <c e bf,>4~
    | % G7
%%%%%%%%%% 4 %%%%%%%%%%%
    <c e bf,>8
    r4
    <e a, c>8~
    <e a, c>4
    <a, b f>4
    | % C7
%%%%%%%%%% 5 %%%%%%%%%%%
    <c a>8
    <bf f d>8 % F
    <bf f b c a>4
    <c f bf>2
    | % gm
%%%%%%%%%% 6 %%%%%%%%%%%
    <d f bf> % F
    <a c e>
    | % em
    <bf, f ef> % Fsus
    <<
    	{ e4 d } \\
    	{ <a, bf,>2 }
    >>
    | % G7 }
    <f, a, c>1 % C
}

chords_db_pt = {
    \accidentalStyle "dodecaphonic"
    \time 5/4
    <<
     	{
   	    <c e>4. <a g>4 <e d>8~
   	    <e d>2
   	} \\
     	{
   	    <d f>2 <a, f>2.
   	}
    >>
    |% em
%%%%%%%%%% 2 4/4 %%%%%%%%%%%
    \time 4/4
    <<
   	{ r8 d4. a2~
	  |
%%%%%%%%%% 3 7/8  %%%%%%%%%%%
  	  \time 7/8
	  a8 a4 s4 s4
      } \\
   	{ <bf d>2 <bf a>
	  |
%%%%%%%%%% 3 7/8  %%%%%%%%%%%
				%  	  \time 7/8
	  <bf d'>4  <bf d'>8
	  [ bf8 <b d>8 ] <a, bf>8 [ <b d>8 ]
      }
   	|
    >>
%%%%%%%%%% 4 4/4 %%%%%%%%%%%
    \time 4/4
    r4 <bf d>4 <bf d c a>4 <f fs b>4~ % F
    | % D7
    %%%%%%%%%% 5 %%%%%%%%%%%
    <f fs b>8 c4 <ef f>8~ <ef f>4 <c e bf,>4
    | % G7
%%%%%%%%%% 6 %%%%%%%%%%%
    <e a, c>2 % C
    <<
	{\tuplet 3/2 { b4 d' c' }}\\
	{<a, f>2}
    >>
    | % C7
%%%%%%%%%% 7 %%%%%%%%%%%  
    <f d>4 bf4
    c4. <f bf>8~
    | % gm
%%%%%%%%%% 8 %%%%%%%%%%%
    \time 5/4
    <f bf>4
    <d f bf>2 % F
    <a c>4
    <a c e f>4~
    | % em
%%%%%%%%%% 9 %%%%%%%%%%%
    <a c e f>4
    <bf f ef a>2 % Fsus
    <<
       	{ e4 d } \\
       	{ <a, bf,>2 }
    >>
    | % G7 }
%%%%%%%%%% 10 %%%%%%%%%%%
    \time 4/4
    <<
	{
	    <e f g>4
	    <e f g>2
	    <e f g>4
	    |
	    \tuplet 5/4 {
		<b, as>4
		<a, c>2
		<f, b>2
	    }
	    
	}\\
	{
	    <a, c >2 % C
	    <a, c >2 % C
	    |
	    <d e>2
	    <f a bf>2
	}
    >>
}

