chords_bass_da_pb =
{
    \clef bass
    \tuplet 3/2 {
	r4 ds,2~
    }
    ds,4.
    e,4.~
    e,8. a,16~
    | %2
    a,4
    bf,8. ef,16~
    \tuplet 5/4 {
	ef,4 b,4 bf,8 
    }
    | %3
    a,4. ef,8~
    ef,8 d,8
    <d e,>4~
    | %4
    <d e,>8
    <<
	{
	    e8
	    \tuplet 3/2
	    {
		f'8 d'4~
	    }
	    d'4
	    c4
	} \\
	{
	    ef,4.~
	    ef,8 e,4.~
	}
    >>
    | %5
    \tuplet 3/2 {
	e,4
	f
	d,~
    }
    d,8 a8~
    a16 ef8.
    | %6
    r8. <d, c>16~
    \tuplet 5/4 {
	<d, c>8
	<ds, d>4
	<e, ds>4
    }
    <d, e>4~
    | %7
    \tuplet 5/4 {
	<d, e>4
	<d, e>2
	<d, e>4.
	<d, e>8~
    }
    | %8
    <d, e>4.
    <d, e>8~
    <d, e>2
}

